{
  "version": 2,
  "rewrites": [
    {
      "source": "/src/(.*)",
      "destination": "/src/$1"
    },
    {
      "source": "/design-system/(.*)",
      "destination": "/design-system/$1"
    },
    {
      "source": "/Asset/(.*)",
      "destination": "/Asset/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}

